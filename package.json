{
  "name": "diet",
  "version": "1.0.0",
  "description": "Awesome web api for diet counter",
  "main": "index.js",
  "scripts": {
    "build": "npx tsc",
    "start": "concurrently \"ts-node -r tsconfig-paths/register src/index.ts -initialData=true\" \"npm run swagger-autogen\"",
    "dev": "concurrently \"npm run sa \" \"ts-node -r tsconfig-paths/register src/index.ts -initialData=true\"",
    "test": "concurrently \"ts-node -r tsconfig-paths/register src/index.ts -initialData=false\" \"jest\"",
    "sa": "node swagger.js"
  },
  "keywords": [
    "api",
    "diet",
    "express",
    "tracking"
  ],
  "author": "KamilNaja",
  "license": "ISC",
  "dependencies": {
    "@types/cookie-session": "^2.0.44",
    "bcrypt": "^5.1.0",
    "cookie-session": "^2.0.0",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "morgan": "^1.10.0",
    "swagger-autogen": "^2.22.0",
    "swagger-ui-express": "^4.6.0",
    "yamljs": "^0.3.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.15",
    "@types/jest": "^29.2.6",
    "@types/morgan": "^1.9.3",
    "@types/node": "^18.11.16",
    "@types/supertest": "^2.0.12",
    "@types/swagger-ui-express": "^4.1.3",
    "@types/yamljs": "^0.2.31",
    "concurrently": "^7.6.0",
    "cross-env": "^7.0.3",
    "nodemon": "^2.0.20",
    "supertest": "^6.3.3",
    "ts-jest": "^29.0.5",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.1.2",
    "typescript": "^4.9.4"
  }
}
