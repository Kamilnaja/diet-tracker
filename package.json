{
  "name": "diet",
  "version": "1.0.0",
  "description": "Awesome web api for diet counter",
  "main": "index.js",
  "scripts": {
    "build": "npx tsc",
    "dev": "concurrently \"npm run sa \" \"ts-node -r tsconfig-paths/register src/index.ts --inspect -initialData=true  \"",
    "test": "concurrently \"ts-node -r tsconfig-paths/register src/index.ts -initialData=false\" \"jest --runInBand \"",
    "test:i": "concurrently \"ts-node -r tsconfig-paths/register src/index.ts --inspect -initialData=false\" \"jest --runInBand \"",
    "sa": "node swagger.js",
    "pt": "ts-node -r tsconfig-paths/register src/index.ts -initialData=false",
    "lint": "eslint --ext .ts src/"
  },
  "keywords": [
    "api",
    "diet",
    "express",
    "tracking"
  ],
  "author": "KamilNaja",
  "license": "ISC",
  "dependencies": {
    "@types/cookie-session": "^2.0.44",
    "bcrypt": "^5.1.0",
    "cookie-session": "^2.0.0",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "jsonwebtoken": "^9.0.0",
    "morgan": "^1.10.0",
    "sql-template-strings": "^2.2.2",
    "sqlite": "^4.1.2",
    "sqlite3": "^5.1.6",
    "swagger-autogen": "2.23.1",
    "swagger-ui-express": "^4.6.0",
    "yamljs": "^0.3.0"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/better-sqlite3": "^7.6.4",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.15",
    "@types/jest": "^29.2.6",
    "@types/jsonwebtoken": "^9.0.2",
    "@types/morgan": "^1.9.3",
    "@types/node": "^18.11.16",
    "@types/supertest": "^2.0.12",
    "@types/swagger-ui-express": "^4.1.3",
    "@types/yamljs": "^0.2.31",
    "@typescript-eslint/eslint-plugin": "^5.59.0",
    "@typescript-eslint/parser": "^5.59.0",
    "concurrently": "^7.6.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.38.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.27.5",
    "nodemon": "^2.0.20",
    "supertest": "^6.3.3",
    "ts-jest": "^29.0.5",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.1.2",
    "typescript": "^4.9.5"
  }
}
